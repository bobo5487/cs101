#include <stdio.h>



int multiplication (int i, int j){

    if (i<=9){

        if (j<=9){

            printf("%d*%d=%2d\t",i,j,i*j);

            multiplication (i,j+1);

            }

        else{

            printf("\n");

            i++;

            multiplication (i,1);

            }   }   }

int main() {

    multiplication (1, 1);

    return 0;

}
